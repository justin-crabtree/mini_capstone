@products.map do |product|
  {
    id: product.id, 
    name: product.name,
    price: product.price, 
    price_data: {
      price: product.price,
      tax: product.tax, 
      total: product.total
    },
    supplier: product.supplier.name, 
    is_discounted?: product.is_discounted?, 
    description: product.description 
    # current_user: current_user
  }


end